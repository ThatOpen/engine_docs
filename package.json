    "generate-docs": "yarn clean && yarn copy-libs && yarn add-tutorials",
    "add-tutorials": "node src/docs-generator/tutorial-generator.mjs",
    "clean": "node src/docs-generator/cleaner.mjs",
    "copy-libs": "node src/docs-generator/code-copier.mjs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "generate": "node src/scripts/doc-generator.mjs",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@mdx-js/react": "^1.6.22",
    "@popperjs/core": "^2.11.7",
    "bim-fragment": "^0.0.17",
    "camera-controls": "^2.3.3",
    "clsx": "^1.2.1",
    "dexie": "^3.2.3",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.150.1",
    "three-mesh-bvh": "^0.5.23",
    "web-ifc": "^0.0.39"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.3.1",
